@article{gamezSegmentacionMateriaBlanca2022a,
 abstract = {"La materia blanca tiene un papel importante en la estructura cerebral, y una lesión o degeneración en ésta área, pude causar daños severos a todo el sistema cerebral, por lo tanto, la segmentación automática de imágenes médicas cerebrales es un proceso importante al utilizar software de apoyo para la detección de lesiones,  neurodegradación o en la evaluación de neurocirugías. En esta investigación, se realizará un estudio comparativo de técnicas para la segmentación automática de la materia blanca cerebral, proponiendo diferentes arquitecturas de segmentación como la U-Net y variantes de la misma. Se observan unas métricas mejores para una U-Net residual que, para las variantes implementadas, teniendo para U-Net un Dice Score de 0.86, para BConvLSTM U-Net un Dice Score de 0.79, para U-Net residual un Dice Score de 0.89 y para U-Net doble un Dice Score de 0.84."},
 annotation = {Accepted: 2022-09-19T17:08:04Z},
 author = {Gamez, Eduardo Daniel Posadas},
 copyright = {http://creativecommons.org/licenses/by-nc-nd/4.0},
 file = {/datos/syphon/lconcha/Zotero/storage/NLTYPJVR/Gamez - 2022 - Segmentación de la Materia Blanca Cerebral por med.pdf},
 langid = {spanish},
 month = {September},
 title = {Segmentación de la Materia Blanca Cerebral por medio de Aprendizaje Automático},
 urldate = {2023-01-24},
 year = {2022}
}

